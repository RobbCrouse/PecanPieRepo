@model pecanpie.Models.UserLogViewModel



<h2>Login</h2>
<br>

<form asp-controller="Home" asp-action="LoginForm" method="Post">
    
    
    <label asp-for="Email"></label>
    <input asp-for="Email"/>
    <br>

    
    <label asp-for="Password"></label>
    <input asp-for="Password"/>
    <br>

    <button type="submit">Login</button>

<h4>@ViewBag.Login</h4>

</form>